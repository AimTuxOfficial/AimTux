    case AntiAimType_Y::JITTER_180:
			clamp = false;
			yFlip ? angle.y = 0.1f : angle.y = 179.0f;
			factor = (globalVars->curtime * 9999.0f);
			break;
		case AntiAimType_Y::BACK_JITTER:
			angle.x = 89.0f;
			clamp = true;
			yFlip ? angle.y = 200.0f : angle.y = 160.0f;
			factor = (globalVars->curtime * 9999999999.0f);			
			break;	
		default:
			angle.y -= 0.0f;
			break;
